{
  "name": "plantuml-parser",
  "version": "1.0.0",
  "description": "Parse PlantUML Syntax in JavaScript",
  "main": "src",
  "dependencies": {
    "get-stdin": "^6.0.0",
    "pegjs-backtrace": "^0.1.2",
    "read-vinyl-file-stream": "^2.0.3",
    "require-dir": "^1.2.0",
    "yars": "^1.2.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "fancy-log": "^1.3.3",
    "gulp": "^4.0.0",
    "gulp-mocha": "^6.0.0",
    "gulp-pegjs": "^0.1.0",
    "gulp-rename": "^1.4.0",
    "mocha": "^6.0.2",
    "nyc": "^13.3.0",
    "pegjs": "^0.10.0"
  },
  "bin": {
    "plantuml-parser": "src/bin/cli.js"
  },
  "scripts": {
    "start": "node src/bin/cli.js",
